{
    "name": "byjoby/formward",
    "description": "PHP tools for constructing, validating, and processing forms. Includes additional tools to construct forms from Digraph DataObjects",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Joby Elliott",
        "email": "joby@byjoby.com"
    }],
    "require": {
        "php": ">=5.3.3"
    },
    "autoload": {
        "psr-4": {
            "Digraph\\Forms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digraph\\Forms\\Tests\\": "tests/"
        }
    },
    "scripts": {
      "test": [
        "phpunit"
      ]
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
